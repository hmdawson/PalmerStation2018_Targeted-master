#Plot field growth curve PC
#------------------------------------------------------------------------------------------------------------

Fielddat <- read_csv("RawOutput/Ancillary/Ant18_TankField_RFUPC.csv")%>%
  filter(TREATMENT == "StationB")

Fieldsumm <- Fielddat %>%
  group_by(DAY, TREATMENT) %>%
  summarise(LN_PC_ave = mean(`LN(PC)`),
            LN_PC_sd = sd(`LN(PC)`),
            PC_ave = mean(`PC`),
            PC_sd = sd(`PC`)) 

